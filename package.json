{
  "name": "we-plugin-page",
  "description": "We.js page plugin with content model",
  "version": "2.0.0",
  "main": "plugin.js",
  "scripts": {
    "test": "NODE_ENV=test LOG_LV=info ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js -b ",
    "verbose-test": "NODE_ENV=test LOG_LV=verbose ./node_modules/.bin/mocha test/bootstrap.js test/**/*.test.js -b ",
    "coverage": "NODE_ENV=test LOG_LV=info node_modules/.bin/istanbul cover -x src node_modules/.bin/_mocha -- -u exports -R spec test/bootstrap.js test/**/*.test.js"
  },
  "devDependencies": {
    "chance": "^1.0.18",
    "connect-sqlite3": "^0.9.11",
    "istanbul": "0.4.5",
    "mocha": "5.2.0",
    "rimraf": "^2.6.3",
    "sinon": "7.2.3",
    "supertest": "3.4.2",
    "we-core": "^2.0.11",
    "we-passport-oauth2-password": "^2.0.1",
    "we-plugin-acl": "^1.2.13",
    "we-plugin-auth": "^2.3.1",
    "we-plugin-file": "^2.0.5",
    "we-plugin-file-local": "^2.1.2",
    "we-plugin-user": "^2.0.1",
    "we-plugin-vocabulary": "^2.0.4",
    "we-test-tools": "wejs/we-test-tools#0.3.21"
  },
  "repository": "wejs/we-plugin-page",
  "author": "Alberto Souza <contato@albertosouza.net>",
  "license": "MIT",
  "keywords": [
    "wejs-plugin"
  ],
  "wejs": {
    "plugins": {
      "we-plugin-auth": true,
      "we-plugin-user": true,
      "we-plugin-file": true,
      "we-plugin-file-local": true,
      "we-plugin-acl": true,
      "we-plugin-vocabulary": true
    },
    "dependencies": {
      "we-plugin-user": true,
      "we-plugin-file": true,
      "we-plugin-vocabulary": true
    }
  }
}
